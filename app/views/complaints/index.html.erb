<p id="notice"><%= notice %></p>

<h1>Listing Complaints</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Middle name</th>
      <th>Last name</th>
      <th>Contact no</th>
      <th>Email</th>
      <th>Area</th>
      <th>Subject</th>
      <th>Last seen date</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @complaints.each do |complaint| %>
      <tr>
        <td><%= complaint.first_name %></td>
        <td><%= complaint.middle_name %></td>
        <td><%= complaint.last_name %></td>
        <td><%= complaint.contact_no %></td>
        <td><%= complaint.email %></td>
        <td><%= complaint.area %></td>
        <td><%= complaint.subject %></td>
        <td><%= complaint.last_seen_date %></td>
        <td><%= complaint.description %></td>
        <td><%= link_to 'Show', complaint %></td>
        <td><%= link_to 'Edit', edit_complaint_path(complaint) %></td>
        <td><%= link_to 'Destroy', complaint, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Complaint', new_complaint_path %>
