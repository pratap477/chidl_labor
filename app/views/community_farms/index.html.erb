<p id="notice"><%= notice %></p>

<h1>Listing Community Farms</h1>

<table>
  <thead>
    <tr>
      <th>Raid</th>
      <th>Department</th>
      <th>Involve member</th>
      <th>String</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @community_farms.each do |community_farm| %>
      <tr>
        <td><%= community_farm.raid %></td>
        <td><%= community_farm.department %></td>
        <td><%= community_farm.involve_member %></td>
        <td><%= community_farm.string %></td>
        <td><%= link_to 'Show', community_farm %></td>
        <td><%= link_to 'Edit', edit_community_farm_path(community_farm) %></td>
        <td><%= link_to 'Destroy', community_farm, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Community farm', new_community_farm_path %>
